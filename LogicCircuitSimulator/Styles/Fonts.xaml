<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:LogicCircuitSimulator">


	<FontFamily x:Key="LatoRegular">ms-appx:///Assets/Fonts/Lato-Regular#Lato</FontFamily>
	<FontFamily x:Key="LatoThin">ms-appx:///Assets/Fonts/Lato-Thin.ttf#Lato</FontFamily>
	<FontFamily x:Key="LatoBold">ms-appx:///Assets/Fonts/Lato-Bold.ttf#Lato</FontFamily>
	<FontFamily x:Key="FontAwesome">ms-appx:///Assets/Fonts/FontAwesome.ttf#FontAwesome</FontFamily>


	<x:Double x:Key="FontSizeSmallest">8</x:Double>
	<x:Double x:Key="FontSizeSmaller">10</x:Double>
	<x:Double x:Key="FontSizeSmall">12</x:Double>
	<x:Double x:Key="FontSizeRegular">14</x:Double>
	<x:Double x:Key="FontSizeBig">17</x:Double>
	<x:Double x:Key="FontSizeLarge">20</x:Double>
	<x:Double x:Key="FontSizeXLarge">24</x:Double>
	<x:Double x:Key="FontSizeXXLarge">30</x:Double>


	


</ResourceDictionary>
